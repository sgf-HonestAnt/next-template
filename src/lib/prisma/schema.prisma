generator client {
  provider = "prisma-client-js"
  output   = "./client"
}

generator zod {
  provider = "prisma-zod-generator"
  output   = "./zod"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Example {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  name  String
  email String
}

model AppUser {
  id    String  @id @default(auto()) @map("_id") @db.ObjectId
  name  String
  email String  @unique
  image String?
}
